        "jane-php/jane-php": "^4.0,<4.5.0",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.13",
        "symfony/phpunit-bridge": "^4.1"
    }
}
